{
  "name": "wikserve",
  "version": "1.0.0",
  "description": "Search Wikipedia in monochrome",
  "main": "index.js",
  "scripts": {
    "start": "watchify src -o build/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "build": "browserify src -o build/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "static": "open index.html",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:evturn/wikserve.git"
  },
  "keywords": [
    "autocomplete",
    "wikipedia",
    "cyclejs",
    "rxjs"
  ],
  "author": "Evan Turner <ev@evturn.com>",
  "license": "ISC",
  "dependencies": {
    "@cycle/core": "^6.0.3",
    "@cycle/dom": "^9.4.0",
    "@cycle/jsonp": "^5.0.1",
    "rx": "^4.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "watchify": "^3.7.0"
  }
}
